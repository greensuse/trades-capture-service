# .gitignore for IntelliJ IDEA + Maven Java project

# IntelliJ IDEA
/.idea/
/*.iml
/.idea/*

# Maven build output
/target/

# Build directories
/out/
/build/
/bin/

# Compiled class files
*.class

# Logs and temporary files
*.log
logs/
tmp/
temp/

# Eclipse project files (if present)
/.classpath
/.project
/.settings/
/.factorypath

# OS files
.DS_Store
Thumbs.db

# VS Code settings
/.vscode/

# Archive and packaging
*.jar
*.war
*.ear

# Misc
.gradle/
